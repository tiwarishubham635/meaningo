{
  "manifest_version": 3,
  "name": "Meaningo",
  "description": "A word meaning chrome extension",
  "version":"0.0.1",
  "icons": {
    "192": "./logo192.png",
    "512": "./logo512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Meaningo!",
    "default_icon": "./logo192.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
